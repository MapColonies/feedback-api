{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "redis": {
    "host": "REDIS_HOST",
    "port": 6379,
    "username": "",
    "password": "",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "ttl": 300
  },
  "kafka": {
    "brokers": "KAFKA_URL:9092",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  },
  "kafkaConsumer": {},
  "kafkaProducer": {},
  "outputTopic": "topic",
  "application": {
    "userValidation": ["@mycompany.net"]
  }
}
